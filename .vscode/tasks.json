{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cmake",
			"label": "build",
			"command": "build",
			"targets": [
				"all"
			],
			"preset": "__defaultBuildPreset__",
			"group": "build",
			"problemMatcher": [
				{
					"owner": "gcc-problem-matcher",
					"pattern": [
						{
							"regexp": "^(.*?):(\\d+):(\\d*):?\\s+(?:fatal\\s+)?(warning|error):\\s+(.*)$",
							"file": 1,
							"line": 2,
							"column": 3,
							"severity": 4,
							"message": 5
						}
					]
				}
			],
			
			"detail": "Use this task to build the project."
		}
	]
}